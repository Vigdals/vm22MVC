@using Microsoft.AspNetCore.Mvc.TagHelpers
@using System.ComponentModel.DataAnnotations.Schema
@using System.Diagnostics
@using System.Security.Cryptography.X509Certificates
@using getAPIstuff.Models
@model UserModel

<div class="text-center">
    <h1 class="display-4">Tippekonk VM22</h1>
    <h6>Tippinga stenger fredag 9.des kl 1500</h6>
    <p></p>
    <form asp-action="Tippekonk">
        <p></p>
        <div class="text-center">
            @{
                var now = DateTime.Now;
                var tippingStartTid = new DateTime(2022, 12, 9, 14, 00, 00);
                if (now < tippingStartTid)
                {
                    <input type="submit" class="btn btn-primary" value="Start tipping" />
                }
                else
                {
                    <p>Tipping er stengt!</p>
                }
            }
        </div>
    </form>
    <p></p>
    <img src="https://upload.wikimedia.org/wikipedia/en/e/e3/2022_FIFA_World_Cup.svg" height="300" />
</div>
